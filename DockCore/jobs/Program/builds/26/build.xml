        ansiColor(&apos;xterm&apos;) {
            stage(&apos;Checkout&apos;) {
                if (!env.hub_org) {
                    println(ANSI_BOLD + ANSI_RED + &quot;Uh Oh! Please set a Jenkins environment variable named hub_org with value as registery/sunbidrded&quot; + ANSI_NORMAL)
